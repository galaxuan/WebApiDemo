@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Get</title>
    <script src="~/Scripts/jquery-1.12.4.js"></script>
    <script>
        $(function () {
            $.ajax({
                type: "get",
                url: "../api/GetAPI/Get",
                data: { id: 1, name: "Jim", time: "1988-09-11" },
                success: function (result, status) {
                    if (status == "success") {
                        $("#result1").html(result);
                    }
                }
            });

            $.ajax({
                type: "get",
                url: "../api/GetAPI/hahaha",
                dataType: "text",
                data: { id: 8888, sex: "男", name: "黄飞鸿", time: "1990-1-1" },
                success: function (result, status) {
                    if (status == "success") {
                        $("#result2").html(result);
                    }
                },
                error: function (result) {
                    alert(result);
                }
            });
        });
    </script>
</head>
<body>
    <div id="result1">
    </div>
    <div id="result2">
    </div>
</body>
</html>